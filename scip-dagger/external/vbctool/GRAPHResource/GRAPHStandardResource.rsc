#determines wether the DisplayArea is displayed or not
DisplayAreaVisible:		False
#determines the ratio of the left and right Displays in percent
DisplayAreaRatio:		30
#if you wish to install the Background-color with colorID:
DrawAreaBackgroundColor:	wheat
#how many values do You wish to install for the LineWidthScale
LineWidthScaleNumberOfValues:	1000
#loads a special File Filter
LoadFileFilter: *
SaveFileFilter: *
#sets the first name of the menue bar button in Algorithms. 
#UserDefinedCommandName: User new command 

#sets the value of the level separation.
TreeLevelSeparationValue:	4
#sets the value of the subtree separation.
TreeSubtreeSeparationValue:	6
#sets the value of the sibling separation.
TreeSiblingSeparationValue:     6
#set the radius of all node categories
TreeNodeRadiusValue:		20
#set the time intervall for the visualization algorithm in milliseconds
VisualizeAlgorithmTimeIntervall:	100

